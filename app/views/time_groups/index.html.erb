<p id="notice"><%= notice %></p>

<h1>Listing Time Groups</h1>

<table>
  <thead>
    <tr>
      <th>Student name</th>
      <th>Month</th>
      <th>Day</th>
      <th>Total min</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @time_groups.each do |time_group| %>
      <tr>
        <td><%= time_group.student_name %></td>
        <td><%= time_group.month %></td>
        <td><%= time_group.day %></td>
        <td><%= time_group.total_min %></td>
        <td><%= link_to 'Show', time_group %></td>
        <td><%= link_to 'Edit', edit_time_group_path(time_group) %></td>
        <td><%= link_to 'Destroy', time_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Time group', new_time_group_path %>
